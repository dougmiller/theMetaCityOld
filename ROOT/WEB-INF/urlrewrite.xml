<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule match-type="regex">
        <from>^/sitemap.xml/?$</from>
        <to>/sitemap.jsp</to>
    </rule>
    <rule match-type="regex">
        <from>^/about/?$</from>
        <to>/about.jsp</to>
    </rule>
    <rule match-type="regex">
        <from>^/twitter/?$</from>
        <to>/twitter.jsp</to>
    </rule>
    <rule match-type="regex">
        <from>^/github/?$</from>
        <to>/github.jsp</to>
    </rule>
    <rule match-type="regex">
        <from>^/licence/?$</from>
        <to>/licence.jsp</to>
    </rule>
</urlrewrite>
