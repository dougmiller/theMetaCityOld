<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule match-type="regex">
        <from>^/sitemap.xml$</from>
        <to>/sitemap.jsp</to>
    </rule>
	
    <rule match-type="regex">
        <from>^/?$</from>
        <to>/index.jsp</to>
    </rule>
    <rule match-type="regex">
        <from>^/([0-9]*)/?$</from>
        <to>/project.jsp?id=$1</to>
    </rule>
</urlrewrite>