<!--
If the person is logged in then let them pass unhindered to the appropriate page.
    Check the session to find a name


Otherwise redirect them to the login page.
    Login redirects back to the page they came from
        What about if thre is no prvious page
            goto the main page.

-->

<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<c:if test="${loggedIn != 'true'}">
    <c:redirect url="login.jsp"/>
</c:if>

