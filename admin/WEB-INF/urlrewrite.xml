<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <!-- About page rules -->
    <rule match-type="regex">
        <from>/about/([\w*](-?[\w*])*)/?</from>
        <to>/about.jsp?author=$1</to>
    </rule>
</urlrewrite>